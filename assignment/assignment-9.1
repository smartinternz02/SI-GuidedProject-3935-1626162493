import cv2
import sys
import numpy as np
cap = cv2.VideoCapture(0)  
if (cap.isOpened()== False): 
  print("Error opening video  file")
while(cap.isOpened()):
  ret, frame = cap.read()
  if ret == True:
    frame=cv2.line(frame,(0,0),(100,100),(0,255,0),5)
    cv2.imshow('Frame', frame)
    if cv2.waitKey(5) & 0xFF == ord('q'):
      break
  else: 
    break
cap.release()
cv2.destroyAllWindows()